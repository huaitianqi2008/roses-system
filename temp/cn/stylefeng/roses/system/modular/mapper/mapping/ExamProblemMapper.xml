<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.roses.system.modular.mapper.ExamProblemMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.roses.system.modular.entity.ExamProblem">
        <id column="id" property="id" />
        <result column="exam_id" property="examId" />
        <result column="problem_type" property="problemType" />
        <result column="problem_title" property="problemTitle" />
        <result column="problem_options" property="problemOptions" />
        <result column="problem_answer" property="problemAnswer" />
        <result column="creater" property="creater" />
        <result column="create_time" property="createTime" />
        <result column="img_url" property="imgUrl" />
        <result column="problem_score" property="problemScore" />
        <result column="difficulty_type" property="difficultyType" />
        <result column="is_del" property="isDel" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, exam_id, problem_type, problem_title, problem_options, problem_answer, creater, create_time, img_url, problem_score, difficulty_type, is_del
    </sql>

</mapper>
